const getterSetterCapitalize = (str) => {
    // De-escape reserved names as we'll add a prefix
    if (str.startsWith('_')) {
        str = str.slice(1);
    }
    if (str.length > 1 && str.charAt(0).toLowerCase() === str.charAt(0) && str.charAt(1).toUpperCase() === str.charAt(1)) {
        return str;
    }
    return str.charAt(0).toUpperCase() + str.slice(1);
};
const capitalized = getterSetterCapitalize(name);
