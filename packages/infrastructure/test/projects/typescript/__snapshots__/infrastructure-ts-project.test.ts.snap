import { Construct } from "constructs";
import { Api } from "../constructs/apis/api";
import { Website } from "../constructs/websites/website";
import { Website2 } from "../constructs/websites/website2";

export class ApplicationStack extends Stack {
  constructor(scope: Construct, id: string, props?: StackProps) {
    super(scope, id, props);

    const userIdentity = new UserIdentity(this, \`\${id}UserIdentity\`);
    const api = new Api(this, "Api", {
      userIdentity,
    });
    new Website(this, "Website", {
      userIdentity,
    });
    new Website2(this, "Website2", {
      userIdentity,
      api,
      api2,
    });
  }
}
