          {
            "exec": "yarn exec nx run-many --target=post-compile --output-style=stream --nx-bail",
            "receiveArgs": true,
          },
        ],
      },
      "post-upgrade": {
        "description": "Runs after upgrading dependencies",
        "name": "post-upgrade",
