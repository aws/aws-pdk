    [ -h \\"$app_path\\" ]
do
    ls=$( ls -ld \\"$app_path\\" )
    link=\${ls#*' -> '}
    case $link in             #(
      /*)   app_path=$link ;; #(
      *)    app_path=$APP_HOME$link ;;
