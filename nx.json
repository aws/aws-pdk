{
  "extends": "@nrwl/workspace/presets/npm.json",
  "plugins": [
    "./.nx/plugins/nx-monorepo-plugin.js"
  ],
  "npmScope": "monorepo",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "useDaemonProcess": false,
        "cacheableOperations": [
          "build",
          "test"
        ]
      }
    }
  },
  "implicitDependencies": {
    "pdk-pipeline-sample-ts": [
      "@aws/aws-pdk-lib"
    ],
    "pdk-pipeline-sample-py": [
      "@aws/aws-pdk-lib"
    ],
    "pdk-pipeline-sample-java": [
      "@aws/aws-pdk-lib"
    ],
    "docs": [
      "@aws/aws-pdk-lib",
      "@aws-prototyping-sdk/build-tools"
    ]
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ],
    "upgrade": [
      {
        "target": "upgrade",
        "projects": "dependencies"
      }
    ]
  },
  "affected": {
    "defaultBase": "mainline"
  },
  "//": "~~ Generated by projen. To modify, edit .projenrc.js and run \"npx projen\"."
}
