{
  "extends": "nx/presets/npm.json",
  "npmScope": "monorepo",
  "affected": {
    "defaultBase": "mainline"
  },
  "plugins": [
    "@jscutlery/semver"
  ],
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*"
    ]
  },
  "targetDefaults": {
    "build": {
      "inputs": [
        "default",
        "^default"
      ],
      "outputs": [
        "{projectRoot}/dist",
        "{projectRoot}/lib",
        "{projectRoot}/build",
        "{projectRoot}/coverage",
        "{projectRoot}/test-reports",
        "{projectRoot}/target",
        "{projectRoot}/cdk.out",
        "{projectRoot}/LICENSE_THIRD_PARTY",
        "{projectRoot}/.jsii"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "release:mainline": {
      "dependsOn": [
        "^release:mainline"
      ]
    },
    "upgrade": {
      "dependsOn": [
        "^upgrade"
      ]
    },
    "release:version": {
      "dependsOn": [
        "^release:version"
      ]
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx-cloud",
      "options": {
        "useDaemonProcess": false,
        "cacheableOperations": [
          "build",
          "test",
          "generate"
        ],
        "accessToken": "OWJmZDJmZmEtNzk5MC00OGJkLTg3YjUtNmNkZDk1MmYxZDZkfHJlYWQ="
      }
    }
  },
  "//": "~~ Generated by projen. To modify, edit .projenrc.ts and run \"npx projen\"."
}
