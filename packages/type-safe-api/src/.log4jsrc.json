{
    "appenders": {
        "out": {
            "type": "stdout",
            "layout": {
                "type": "pattern",
                "pattern": "%[[%d{yyyy-MM-dd-hh:mm:ss}] [%p] [%f{1}] -%] %m"
            }
        }
    },
    "categories": {
        "default": { "appenders": [ "out" ], "level": "INFO", "enableCallStack": true }
    }
}